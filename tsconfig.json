{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        // "target": "es2017",
        // "lib": [
        //     "es2017",
        //     "dom"
        // ],
        "noImplicitAny": false,
        "strict": true,
        "inlineSources": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "*": [
                "node_modules/*"
            ]
        }
    },
    "include": [
        "server/**/*"
    ]
}