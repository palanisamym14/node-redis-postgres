{
    "name": "node-react-redis-sms",
    "version": "1.0.0",
    "description": "Node server with postgress db",
    "author": "Palanisamy M",
    "private": true,
    "main": "dist/index.js",
    "scripts": {
        "start": "concurrently --kill-others \"npm run watch\" \"npm run serve\" ",
        "watch": "npm run tsc:w",
        "tsc:w": "tsc -w -p ./tsconfig.json",
        "tsc": "tsc -w -p ./tsconfig.json && node dist/index.js",
        "serve": "node dist/index.js",
        "build": "npm run prestart",
        "prestart": "npm run prebuild",
        "start-build": "tsc -p ./tsconfig.json && node dist/index.js",
        "prebuild": "tsc -p ./tsconfig.json",
        "clusterserve": "node dist/cluster.js",
        "clusterserve:prod": "export set NODE_ENV=production && npm run clusterserve"
    },
    "gypfile": true,
    "dependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/compression": "0.0.36",
        "@types/debug": "4.1.0",
        "@types/dotenv": "^6.1.0",
        "@types/es6-promise": "^3.3.0",
        "@types/jsonwebtoken": "^8.3.0",
        "@types/lodash": "^4.14.120",
        "@types/uuid": "^3.4.4",
        "async": "^2.6.1",
        "axios": "^0.18.0",
        "bcrypt": "^3.0.3",
        "bindings": "^1.4.0",
        "bluebird": "^3.5.3",
        "body-parser": "^1.18.3",
        "cluster": "^0.7.7",
        "compression": "^1.7.3",
        "concurrently": "^4.0.1",
        "cors": "^2.8.5",
        "cpx": "^1.5.0",
        "dotenv": "^6.1.0",
        "joi": "^14.3.1",
        "jsonwebtoken": "^8.4.0",
        "lodash": "^4.17.11",
        "nan": "^2.12.1",
        "node-fetch": "^2.3.0",
        "os": "^0.1.1",
        "path": "^0.12.7",
        "pg": "^7.12.1",
        "redis": "^2.8.0",
        "sequelize": "^4.44.3",
        "tedious": "^4.1.3",
        "ts-node": "^8.0.2"
    },
    "devDependencies": {
        "@types/body-parser": "^1.16.5",
        "@types/chalk": "^2.2.0",
        "@types/cors": "^2.8.1",
        "@types/express": "^4.16.0",
        "@types/node": "^10.12.18",
        "express": "^4.16.4",
        "ncp": "^2.0.0",
        "nodemon": "^1.18.6",
        "shelljs": "^0.8.3",
        "tslint": "^5.7.0",
        "typescript": "^3.2.2"
    },
    "engines": {
        "node": ">= 4.0.0",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
